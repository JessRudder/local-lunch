<%= form_for restaurant do |f| %>
  <%= f.hidden_field :community_id %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :category %>
  <%= f.text_field :category %>
  <!-- Probably want to replace this when we have a better way to add restaurants -->
  <%= f.label :yelp_url %>
  <%= f.text_field :yelp_url %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.submit %>
<% end %>